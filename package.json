{
  "name": "@all-user/ok-blocks-demos",
  "version": "0.0.3",
  "description": "OK Blocks Demos.",
  "main": "index.js",
  "scripts": {
    "help": "npm-run-help",
    "build:common:css": "stylus -c ./src/stylus/common.styl -o ./src/demos/common.css -m",
    "build:common:js": "browserify -r @all-user/ok-blocks -r @all-user/ok-patterns-lines -r @all-user/ok-patterns-olympic2020 -t babelify -o ./src/demos/common.js -v",
    "build:common": "npm-run-all -p build:common:*",
    "watch:common:css": "stylus -w -c ./src/stylus/common.styl -o ./src/demos/common.css -m",
    "watch:common:js": "watchify -r @all-user/ok-blocks -r @all-user/ok-patterns-lines -r @all-user/ok-patterns-olympic2020 -t babelify -o ./src/demos/common.js -v",
    "watch:common": "npm-run-all -p watch:common:*",
    "build:demos:lines:all_char": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/all_char_animate.js -o ./src/demos/Lines/all_char_animate/bundle.js; stylus -c ./src/stylus/all_char_animate.styl -o ./src/demos/Lines/all_char_animate/bundle.css -m",
    "watch:demos:lines:all_char": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/all_char_animate.js -o ./src/demos/Lines/all_char_animate/bundle.js -v & stylus -w -c ./src/stylus/all_char_animate.styl -o ./src/demos/Lines/all_char_animate/bundle.css -m",
    "build:demos:lines:str_anime": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/string_animate.js -o ./src/demos/Lines/string_animate/bundle.js; stylus -c ./src/stylus/string_animate.styl -o ./src/demos/Lines/string_animate/bundle.css -m",
    "watch:demos:lines:str_anime": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/string_animate.js -o ./src/demos/Lines/string_animate/bundle.js -v & stylus -w -c ./src/stylus/string_animate.styl -o ./src/demos/Lines/string_animate/bundle.css -m",
    "build:demos:lines:frm_txt": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/animate_from_input.js -o ./src/demos/Lines/animate_from_input/bundle.js; stylus -c ./src/stylus/animate_from_input.styl -o ./src/demos/Lines/animate_from_input/bundle.css -m",
    "watch:demos:lines:frm_txt": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/animate_from_input.js -o ./src/demos/Lines/animate_from_input/bundle.js -v & stylus -w -c ./src/stylus/animate_from_input.styl -o ./src/demos/Lines/animate_from_input/bundle.css -m",
    "build:demos:lines:embed": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/embed.js -o ./src/demos/Lines/embed/bundle.js; stylus -c ./src/stylus/embed.styl -o ./src/demos/Lines/embed/bundle.css -m",
    "watch:demos:lines:embed": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/embed.js -o ./src/demos/Lines/embed/bundle.js -v & stylus -w -c ./src/stylus/embed.styl -o ./src/demos/Lines/embed/bundle.css -m",
    "build:demos:lines:embed_res": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/embed_response.js -o ./src/demos/Lines/embed_response/bundle.js; stylus -c ./src/stylus/embed_response.styl -o ./src/demos/Lines/embed_response/bundle.css -m",
    "watch:demos:lines:embed_res": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-lines -t babelify ./src/js/Lines/embed_response.js -o ./src/demos/Lines/embed_response/bundle.js -v & stylus -w -c ./src/stylus/embed_response.styl -o ./src/demos/Lines/embed_response/bundle.css -m",
    "build:demos:olympic:all_char": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/all_char_animate.js -o ./src/demos/Olympic2020/all_char_animate/bundle.js; stylus -c ./src/stylus/all_char_animate.styl -o ./src/demos/Olympic2020/all_char_animate/bundle.css -m",
    "watch:demos:olympic:all_char": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/all_char_animate.js -o ./src/demos/Olympic2020/all_char_animate/bundle.js -v & stylus -w -c ./src/stylus/all_char_animate.styl -o ./src/demos/Olympic2020/all_char_animate/bundle.css -m",
    "build:demos:olympic:str_anime": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/string_animate.js -o ./src/demos/Olympic2020/string_animate/bundle.js; stylus -c ./src/stylus/string_animate.styl -o ./src/demos/Olympic2020/string_animate/bundle.css -m",
    "watch:demos:olympic:str_anime": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/string_animate.js -o ./src/demos/Olympic2020/string_animate/bundle.js -v & stylus -w -c ./src/stylus/string_animate.styl -o ./src/demos/Olympic2020/string_animate/bundle.css -m",
    "build:demos:olympic:frm_txt": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/animate_from_input.js -o ./src/demos/Olympic2020/animate_from_input/bundle.js; stylus -c ./src/stylus/animate_from_input.styl -o ./src/demos/Olympic2020/animate_from_input/bundle.css -m",
    "watch:demos:olympic:frm_txt": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/animate_from_input.js -o ./src/demos/Olympic2020/animate_from_input/bundle.js -v & stylus -w -c ./src/stylus/animate_from_input.styl -o ./src/demos/Olympic2020/animate_from_input/bundle.css -m",
    "build:demos:olympic:embed": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/embed.js -o ./src/demos/Olympic2020/embed/bundle.js; stylus -c ./src/stylus/embed.styl -o ./src/demos/Olympic2020/embed/bundle.css -m",
    "watch:demos:olympic:embed": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/embed.js -o ./src/demos/Olympic2020/embed/bundle.js -v & stylus -w -c ./src/stylus/embed.styl -o ./src/demos/Olympic2020/embed/bundle.css -m",
    "build:demos:olympic:embed_res": "browserify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/embed_response.js -o ./src/demos/Olympic2020/embed_response/bundle.js; stylus -c ./src/stylus/embed_response.styl -o ./src/demos/Olympic2020/embed_response/bundle.css -m",
    "watch:demos:olympic:embed_res": "watchify -x @all-user/ok-blocks -x @all-user/ok-patterns-olympic2020 -t babelify ./src/js/Olympic2020/embed_response.js -o ./src/demos/Olympic2020/embed_response/bundle.js -v & stylus -w -c ./src/stylus/embed_response.styl -o ./src/demos/Olympic2020/embed_response/bundle.css -m",
    "vendor:copy": "cpx -L \"./node_modules/@all-user/*/dist/**/*\" ./src/vendor/@all-user & cpx \"./node_modules/bootstrap/dist/**/*\" ./src/vendor/bootstrap/dist",
    "vendor": "npm run vendor:copy",
    "build:demos": "npm-run-all -p build:demos:**:*",
    "watch:demos": "npm-run-all -p watch:demos:**:*",
    "release:clean": "rimraf ./dist",
    "release:copy": "cpx \"./src/demos/**/{*.js,*.css,*.html,*.eot,*.svg,*.ttf,*.woff,*.woff2}\" ./dist/demos; cpx \"./src/vendor/**/*\" ./dist/vendor; cp ./src/index.html ./dist",
    "release": "npm-run-all -s build vendor release:clean release:copy",
    "server": "browser-sync start --server src",
    "build": "npm-run-all -p build:demos build:common",
    "watch": "npm-run-all -p watch:demos watch:common",
    "start": "npm-runall -p vendor watch server"
  },
  "author": "Keita Okamoto <ok@aaa.email.ne.jp>",
  "license": "MIT",
  "devDependencies": {
    "@all-user/ok-blocks": "0.0.6",
    "@all-user/ok-patterns-lines": "0.0.3",
    "@all-user/ok-patterns-olympic2020": "0.0.3",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "browser-sync": "^2.11.2",
    "browserify": "^13.0.0",
    "cpx": "^1.3.1",
    "eslint": "^2.7.0",
    "nib": "^1.1.0",
    "npm-run-all": "^1.7.0",
    "npm-run-help": "^1.1.0",
    "rimraf": "^2.5.2",
    "stylus": "^0.54.2",
    "uglify-js": "^2.6.2",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "xtend": "^4.0.1"
  }
}
